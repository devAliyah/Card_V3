<mvc:View
	controllerName="test.controller.Page"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">

	 <Page id="page" title="{i18n>pagetitle}">

	<FeedInput
		post="onPost"
		icon="test-resources/sap/m/images/dronning_victoria.jpg"
		class="sapUiSmallMarginTopBottom" />
	<List
		showSeparators="Inner"
		items="{review>/EntryCollection}" >
		<FeedListItem
			sender="{Author}"
			icon="{AuthorPicUrl}"
			senderPress="onSenderPress"
			iconPress="onIconPress"
			iconDensityAware="false"
			info="{Type}"
			timestamp="{Date}"
			text="{Text}"
			convertLinksToAnchorTags="All"/>
	</List>

	</Page>
</mvc:View>